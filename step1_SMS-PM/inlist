! This is the first inlist file that MESA reads when it starts.

! This file tells MESA to go look elsewhere for its configuration
! info. This makes changing between different inlists easier, by
! allowing you to easily change the name of the file that gets read.

&star_job

    read_extra_star_job_inlist(1) = .true.
    extra_star_job_inlist_name(1) = '../inlists/inlist_star_common'

    read_extra_star_job_inlist(2) = .true.
    extra_star_job_inlist_name(2) = 'inlist_star'

    read_extra_star_job_inlist(3) = .true.
    extra_star_job_inlist_name(3) = 'inlist_star_step1'

/ ! end of star_job namelist


&eos

    read_extra_eos_inlist(1) = .true.
    extra_eos_inlist_name(1) = '../inlists/inlist_star_common'

    !read_extra_eos_inlist(3) = .true.
    !extra_eos_inlist_name(3) = 'inlist_star'

    read_extra_eos_inlist(2) = .true.
    extra_eos_inlist_name(2) = 'inlist_star_step1'

/ ! end of eos namelist


&kap

    read_extra_kap_inlist(1) = .true.
    extra_kap_inlist_name(1) = '../inlists/inlist_star_common'

    read_extra_kap_inlist(2) = .true.
    extra_kap_inlist_name(2) = 'inlist_star'

    read_extra_kap_inlist(3) = .true.
    extra_kap_inlist_name(3) = 'inlist_star_step1'

/ ! end of kap namelist


&controls

    read_extra_controls_inlist(1) = .true.
    extra_controls_inlist_name(1) = '../inlists/inlist_star_common'

    read_extra_controls_inlist(2) = .true.
    extra_controls_inlist_name(2) = 'inlist_star'

    read_extra_controls_inlist(3) = .true.
    extra_controls_inlist_name(3) = '../inlists/inlist_x_ctrls_POSYDON'

    read_extra_controls_inlist(4) = .true.
    extra_controls_inlist_name(4) = 'inlist_star_step1'

/ ! end of controls namelist


&pgstar

    !read_extra_pgstar_inlist(1) = .true.
    !extra_pgstar_inlist_name(1) = '../inlists/inlist_star_common'

    !read_extra_pgstar_inlist(2) = .true.
    !extra_pgstar_inlist_name(2) = 'inlist_pgstar'

    !read_extra_pgstar_inlist(2) = .true.
    !extra_pgstar_inlist_name(2) = 'inlist_star'

/ ! end of pgstar namelist
